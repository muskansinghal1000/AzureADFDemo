		{
			"name": "[concat(parameters('factoryName'), '/dataflow1')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "DS_SqlServerfordataflow",
								"type": "DatasetReference"
							},
							"name": "sqltable"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "DS_TransformSql_into_Adls_DF",
								"type": "DatasetReference"
							},
							"name": "sinkInAdls"
						}
					],
					"transformations": [
						{
							"name": "countryindia",
							"description": "Add expression for the filter"
						},
						{
							"name": "rankedduplicatesAndCountTotalProdKey"
						},
						{
							"name": "rank1"
						},
						{
							"name": "selectcolumns"
						}
					],
					"scriptLines": [
						"source(output(",
						"          OrderDate as string,",
						"          ProductKey as string,",
						"          Country as string,",
						"          EnglishProductName as string,",
						"          SalesAmount as string,",
						"          TaxAmt as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> sqltable",
						"sqltable filter(Country=='India') ~> countryindia",
						"countryindia window(over(ProductKey),",
						"     asc(ProductKey, true),",
						"     rank = rowNumber(),",
						"          {Total Sold} = count(ProductKey)) ~> rankedduplicatesAndCountTotalProdKey",
						"rankedduplicatesAndCountTotalProdKey filter(rank==1) ~> rank1",
						"rank1 select(mapColumn(",
						"          ProductKey,",
						"          Country,",
						"          EnglishProductName,",
						"          SalesAmount,",
						"          {Total Sold}",
						"     ),",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> selectcolumns",
						"selectcolumns sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     umask: 0022,",
						"     preCommands: [],",
						"     postCommands: [],",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> sinkInAdls"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/DS_SqlServerfordataflow')]",
				"[concat(variables('factoryId'), '/datasets/DS_TransformSql_into_Adls_DF')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/dataflow_JoinSqltable')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "DS_SqlEmpData",
								"type": "DatasetReference"
							},
							"name": "Emptable"
						},
						{
							"dataset": {
								"referenceName": "DS_SqlDepttable",
								"type": "DatasetReference"
							},
							"name": "DeptTable"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "DS_JoinInDF",
								"type": "DatasetReference"
							},
							"name": "sink1"
						}
					],
					"transformations": [
						{
							"name": "join1"
						},
						{
							"name": "select1"
						},
						{
							"name": "sort1"
						}
					],
					"scriptLines": [
						"source(output(",
						"          empid as integer,",
						"          name as string,",
						"          sal as decimal(7,2),",
						"          deptno as integer",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> Emptable",
						"source(output(",
						"          deptno as integer,",
						"          deptname as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> DeptTable",
						"Emptable, DeptTable join(Emptable@deptno == DeptTable@deptno,",
						"     joinType:'inner',",
						"     matchType:'exact',",
						"     ignoreSpaces: false,",
						"     broadcast: 'auto')~> join1",
						"join1 select(mapColumn(",
						"          empid,",
						"          name,",
						"          sal,",
						"          deptno = Emptable@deptno,",
						"          deptname",
						"     ),",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> select1",
						"select1 sort(asc(empid, true)) ~> sort1",
						"sort1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          ProductKey as string,",
						"          Country as string,",
						"          EnglishProductName as string,",
						"          SalesAmount as string,",
						"          {Total Sold} as string",
						"     ),",
						"     umask: 0022,",
						"     preCommands: [],",
						"     postCommands: [],",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     partitionBy('hash', 1)) ~> sink1"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/DS_SqlEmpData')]",
				"[concat(variables('factoryId'), '/datasets/DS_SqlDepttable')]",
				"[concat(variables('factoryId'), '/datasets/DS_JoinInDF')]"
			]
		},
